<%- include("header",{element: element}); -%>


<!-- Page-Title -->
<div class="row">
    <div class="col-sm-12">
        <div class="page-title-box">
            <div class="float-right align-item-center mt-2">
                <button  href="#custom-modal-refer-accept"
                    class="btn btn-info px-4 align-self-center report-btn" data-animation="blur"
                    data-plugin="custommodal" data-overlaycolor="#38414a" title="Discharge">Add Oxygen Data</button>
            </div>
            <h4 class="page-title mb-2"><i class="mdi mdi-monitor mr-2"></i>Oxygen Management</h4>
            <div class="">
                <ol class="breadcrumb">
                    <li class="breadcrumb-item"><a href="javascript:void(0);">District Covid Management</a></li>
                    <li class="breadcrumb-item active">Oxygen Management</li>
                </ol>
            </div>
        </div>
    </div>
</div>
<!-- end page title end breadcrumb -->
<%- include("header2"); -%>
<!-- Page Content-->
<div class="page-content" style="overflow: scroll;">
        <div class="container" style="overflow: scroll;">

        <div class="row">


            <div class="col-lg-12">
                <div class="card">
                    <div class="card-body">
                        <h4 class="mt-0 header-title">Previous Referrals</h4>
                        <table id="datatable-buttons" class="table table-striped table-bordered dt-responsive "
                            style="border-collapse: collapse; border-spacing: 0; width: 100%;">
                            <thead class="thead-light">
                                <tr>
                                    <th>Date</th>
                                    <th>Total B Type (Small) Oxygen Available</th>
                                    <th>Total B Type (Small) Oxygen Used Yesterday</th>
                                    <th>Total B Type (Small) Oxygen Received Yesterday</th>
                                    <th>Total D Type (Jumbo) Oxygen Available</th>
                                    <th>Total D Type (Jumbo) Oxygen Used Yesterday</th>
                                    <th>Total D Type (Jumbo) Oxygen Received Yesterday</th>
                                   
                                    
                                </tr>
                            </thead>
                            <tbody>
                                <% oxygen_data.forEach(element => { %>
                                    <tr>
                                        <td> <%= element.date %> </td>
                                        <td>  <%= element.b_total_oxygen_available %>
                                        <td> <%= element.b_total_oxygen_used_yesterday %> </td>
                                        <td> <%= element.b_total_oxygen_received_yesterday %> </td>
                                        <td>  <%= element.d_total_oxygen_available %></td>
                                            <td> <%= element.d_total_oxygen_used_yesterday %> </td>
                                            <td> <%= element.d_total_oxygen_received_yesterday %> </td>
                                        
                                    </tr>
                                <% }) %>
                                
                            </tbody>
                        </table>
                        <!--end /table-->
                    </div>
                    <!--end /tableresponsive-->
                </div>
                <!--end card-body-->
            </div>
            <!--end card-->
        </div> <!-- end col -->
    </div> <!-- end row -->


    <div id="custom-modal-refer-accept" class="modal-demo">
        <button type="button" class="close" onclick="Custombox.modal.close();"><span>Ã—</span><span
                class="sr-only">Close</span></button>
        <div class="card">
            <div class="card-body">
                <h4 class="mt-0 header-title">Add Data (Unit Liters)</h4>
                <div class="row">
                    <div class="col-lg-12">
                        <form action="/add_oxygen_data" method="post">

                            <div class="form-group row">
                                <label for="example-text-input" class="col-sm-2 col-form-label text-right">Date</label>
                                <div class="col-sm-10">
                                    <input class="form-control" type="date" name="date" id="r_id_refer_accept"
                                         required>
                                </div>
                            </div>


                            <div class="form-group row">
                                <label for="example-email-input" class="col-sm-6 col-form-label text-right">Total B type (Small) Available Oxygen </label>
                                <div class="col-sm-6">
                                    <input class="form-control" type="number" name="b_total_oxygen_available" id="example-tel-input"
                                        cols="30" rows="2" required>
                                </div>
                            </div>
                            <div class="form-group row">
                                <label for="example-email-input" class="col-sm-6 col-form-label text-right">Total  B type (Small) Oxygen Used Yesterday</label>
                                <div class="col-sm-6">
                                    <input class="form-control" type="number" name="b_total_oxygen_used_yesterday" id="example-tel-input"
                                        cols="30" rows="2" required>
                                </div>
                            </div>
                            <div class="form-group row">
                                <label for="example-email-input" class="col-sm-6 col-form-label text-right">Total  B type (Small) Oxygen Received Yesterday</label>
                                <div class="col-sm-6">
                                    <input class="form-control" type="number" name="b_total_oxygen_received_yesterday" id="example-tel-input"
                                        cols="30" rows="2" required>
                                </div>
                            </div>
                            <div class="form-group row">
                                <label for="example-email-input" class="col-sm-6 col-form-label text-right">Total D type (Jumbo) Available Oxygen </label>
                                <div class="col-sm-6">
                                    <input class="form-control" type="number" name="d_total_oxygen_available" id="example-tel-input"
                                        cols="30" rows="2" required>
                                </div>
                            </div>
                            <div class="form-group row">
                                <label for="example-email-input" class="col-sm-6 col-form-label text-right">Total D type (Jumbo) Oxygen Used Yesterday</label>
                                <div class="col-sm-6">
                                    <input class="form-control" type="number" name="d_total_oxygen_used_yesterday" id="example-tel-input"
                                        cols="30" rows="2" required>
                                </div>
                            </div>
                            <div class="form-group row">
                                <label for="example-email-input" class="col-sm-6 col-form-label text-right">Total D type (Jumbo) Oxygen Received Yesterday</label>
                                <div class="col-sm-6">
                                    <input class="form-control" type="number" name="d_total_oxygen_received_yesterday" id="example-tel-input"
                                        cols="30" rows="2" required>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-sm-12 text-right">
                                    <button type="submit" class="btn btn-primary">Add Data</button>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
            <!--end card-body-->
            <!--end card-body-->
        </div>
    </div>


</div><!-- container -->

<script>
    function setreferacceptmodal(r_id_refer_accept) {
        document.getElementById("r_id_refer_accept").value = r_id_refer_accept;
        document.getElementById("rrt_id_refer_accept").value = r_id_refer_accept;
        document.getElementById("r_id_refer_reject").value = r_id_refer_accept;
        document.getElementById("r_id_refer_discharge").value = r_id_refer_accept;

    }
</script>
</div>
<%- include("footer"); -%>