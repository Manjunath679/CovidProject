        function get_available_bed_catagory(center_id) {
            console.log(center_id);
            var xhttp = new XMLHttpRequest();
            xhttp.onreadystatechange = function () {
                if (this.readyState == 4 && this.status == 200) {
                    var b_dt = JSON.parse(this.responseText);
                    doms = document.getElementById("set-bed-category-list");
                        var length = doms.options.length;
                        for (ji = length-1; ji >= 0; ji--) {
                            doms.options[ji] = null;
                        }
                      
                    OBA=false
                    GBA=false
                    HBA=false
                    IBA=false
                    VBA=false
                    var opt = document.createElement("option");
                    opt.value = "";
                    opt.text = "Select";
                doms.appendChild(opt);
                    for (var i = 0; i < b_dt.hospital_list.length; i++) {
                        element = b_dt.hospital_list[i];
                       

                    // total_bed_available: Number(element.no_of_general_beds + element.no_of_oxygen_beds + element.no_of_icu_beds + element.no_of_ventilator_beds + element.no_isolation_beds + element.no_hdu_beds),
                    // total_oxygen_bed_available: element.no_of_oxygen_beds - OXYGEN_BED_COUNT,
                    // total_general_bed_available: element.no_of_general_beds - GENERAL_BED_COUNT,
                    // total_icu_bed_available: element.no_of_ventilator_beds+element.no_of_icu_beds - ICU_BED_COUNT,
                    // total_ventilator_bed_available: element.no_of_ventilator_beds - VENTILATOR_BED_COUNT,
                    // total_hdu_bed_available: element.no_hdu_beds - HDU_BED_COUNT,

                        if(element.total_bed_available>=1){}
                        if(!OBA && element.total_oxygen_bed_available>=1){
                            OBA=true
                            var option = document.createElement("option");
                        option.value = "OXYGEN_BED";
                        option.text = "GENERAL BED WITH OXYGEN";
                        doms.appendChild(option);

                        }
                        if(!GBA &&element.total_general_bed_available>=1){
                            GBA=true
                            var option = document.createElement("option");
                        option.value = "GENERAL_BED";
                        option.text = "GENERAL BED WITH OUT OXYGEN";
                        doms.appendChild(option);
                        }
                        if(!IBA &&element.total_icu_bed_available>=1){
                            IBA=true
                            var option = document.createElement("option");
                        option.value = "ICU_BED";
                        option.text = "ICU BED WITH OUT VENTELATOR";
                        doms.appendChild(option);
                        }
                        if(!VBA &&element.total_ventilator_bed_available>=1){
                            VBA=true
                            var option = document.createElement("option");
                        option.value = "VENTILATOR_BED";
                        option.text = "ICU BED WITH VENTILATOR";
                        doms.appendChild(option);
                        }
                        if(!HBA &&element.total_hdu_bed_available>=1){
                            HBA=true
                            var option = document.createElement("option");
                        option.value = "HDU_BED";
                        option.text = "HDU BED(Highly Dependent Unit)";
                        doms.appendChild(option);
                        }
                        

                    }

                    // for (var i = 0; i < b_dt.hospital_list.length; i++) {
                    //     element = b_dt.hospital_list[i];
                    //     doms = document.getElementById("get-section-list");
                        
                    //     var option = document.createElement("option");
                    //     option.value = element.section_id;
                    //     option.text = element.section_id + "-" + element.section_details;
                    //     doms.appendChild(option);

                    // }


                } else {
                    var b_dt = JSON.parse(this.responseText);
                }
            };
            xhttp.open("GET", "/api/bed-details/" + center_id, true);
            xhttp.send();



        }
    


