                                        <li>Bed No.: <%= element.active_bed_details.bed_no%></li>
                                        <li><%= element.active_bed_details.bed_category%></li>
                                        <li><%= element.active_bed_details.center_details.split("-")[0]%></li>
                                        
                                        <a onclick="setrefermodal('<%= element.patient_id%>', '<%= element.patient_name%>', '<%= element.active_bed_details.center_id%>')" type="button" style="color:#007bff;" data-toggle="modal"
