                            style="border-collapse: collapse; border-spacing: 0; width: 100%;">
                            <thead>
                                <tr>
                                    <th>Patient Details</th>
                                    
                                    <th>Current Health</th>
                                    <th>Status</th>
                                    <th>Center Details</th>
                                    <th>Section Details</th>
                                    <th>Bed Details</th>
                                    <th>Family Telephone</th>
                                    <th>Telephone</th>
                                    <th>Symptoms</th>
                                    <th>Age</th>
                                    <th>Sex</th>
                                    <th>S/D/F/W Of</th>
                                    <th>Adress</th>
                                    <th>Village</th>
                                    <th>Block</th>
                                    <th>Covid Status</th>
                                    <th>Covid Report</th>
                                    <th>Co Morbidities</th>
                                    <th>Travel History</th>
                                    <th>Positive Declare Date</th>
                                    <th>Admission Date</th>
                                    <th>Edit Patient</th>
                                    

                                </tr>
                            </thead>
                            <tbody>
                                <% patients_list.forEach(element => { if(element.discharge_status==="ACTIVE") { %>
                                <tr>
                                    <td> <%= element.patient_name%> <br> <%= element.patient_id%></td>
                                    <td>Date: <%= element.active_health_status.date%> <br>
                                        Time: <%= element.active_health_status.time%> <br>
                                        Bp: <%= element.active_health_status.bp%> <br>
                                        SpO2: <%= element.active_health_status.spo2%> <br>
                                        Pulse: <%= element.active_health_status.pulse%> <br>
                                        FBS: <%= element.active_health_status.fbs%> <br>
                                        RBS: <%= element.active_health_status.rbs%> <br>
                                        Temperature: <%= element.active_health_status.temperature%> <br>
                                        Condition: <%= element.active_health_status.condition%> <br>
                                        <a href="/patientdetails/<%= element.patient_id%>">See More</a> <br>
                                    </td>
                                    <td><%= element.discharge_status%></td>
                                    <td><%= element.active_bed_details.center_id%> <br><%= element.active_bed_details.center_details%></td>
                                    <td><%= element.active_bed_details.section_id%> <br><%= element.active_bed_details.section_details%></td>
                                    <td><%= element.active_bed_details.bed_no%> <br><%= element.active_bed_details.bed_category%></td>
                                    <td><%= element.alt_telephone%></td>
                                    <td><%= element.telephone%></td>
                                    <td><%= element.symptoms%></td>
                                    <td><%= element.age%></td>
                                    <td><%= element.sex%></td>
                                    <td><%= element.s_d_w_of%></td>
                                    <td><%= element.address%></td>
                                    <td><%= element.village%></td>
                                    <td><%= element.block%></td>
                                    <td><%= element.covid_status%></td>
                                    <td><%= element.covid_report%></td>
                                    <td><%= element.co_morbidities%></td>
                                    <td><%= element.travel_history%></td>
                                    <td><%= element.positive_declare_date%></td>
                                    <td><%= element.admission_date%></td>
                                    <td><a type="button" class="btn btn-info-outline px-4 align-self-center report-btn" data-toggle="modal"
                                        data-target=".bd-example-modal-xl-edit">Edit </a></td>


                                </tr>
                               

